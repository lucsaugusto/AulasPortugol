programa
{
	
	funcao inicio()
	{
		inteiro contador = 0

		escreva("- Crescente -")
		faca
		{
			contador = contador + 1
			escreva("\n", contador)
		}
		enquanto (contador < 10)

		escreva("\n\n- Decrescente -")
		faca 
		{
			escreva("\n", contador)
			contador = contador - 1
		}
		enquanto (contador > 0)
	}
}